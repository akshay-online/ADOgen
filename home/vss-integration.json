{
    "manifestVersion": 1,
    "id": "DemoGenerator",
    "version": "1.10.1",
    "name": "Azure DevOps Generator",
    "publisher": "akshayh",
    "description": "Azure DevOps Generator helps you create projects on your Azure DevOps Organization with pre-populated templates",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud.Integration"
        }
    ],
    "galleryFlags": [
        "private"
    ],
    "icons": {
        "default": "images/logo.png"
    },
    "categories": [
        "Azure Boards",
        "Azure Repos",
        "Azure Pipelines",
        "Azure Test Plans"
    ],
    "tags": [
        "Azure DevOps",
        "Azure DevOps Generator",
        "Azure DevOps Templates",
        "Azure DevOps Project",
        "Azure DevOps Demo Generator",
        "Demo Generator"
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "images": [
        {
            "path": "AppDetails.png",
            "behavior": "AlwaysInclude"
        },
        {
            "path": "ChooseAPI.png",
            "behavior": "AlwaysInclude"
        },
        {
            "path": "ChoosePermission.png",
            "behavior": "AlwaysInclude"
        }
    ],
    "licensing": {
        "overrides": [
            {
                "id": "AzureDevOpsGenerator",
                "behavior": "AlwaysInclude"
            }
        ]
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/akshay-online/AzureDevOpsGenerator"
        },
        "support": {
            "uri": "https://github.com/akshay-online/AzureDevOpsGenerator/issues"
        }
    },
    "branding": {
        "color": "blue",
        "theme": "dark"
    }
}